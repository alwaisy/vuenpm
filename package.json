{
  "name": "vuenpm",
  "version": "0.0.1",
  "description": "To list all the vuejs npm packages at one place",
  "productName": "Vue Npm",
  "author": "Awais alwaisy <awaisalwaisy@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "build": "quasar build",
    "doSSR": "quasar mode add ssr",
    "build:ssr": "quasar build -m ssr --debug",
    "start:dev": "quasar dev -m ssr",
    "start:prod": "node dist/ssr",
    "start:spa": "quasar serve --history"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "appwrite": "^13.0.0",
    "axios": "^1.2.1",
    "date-fns": "^2.30.0",
    "markdown-vue": "^1.0.3",
    "pinia": "^2.0.11",
    "quasar": "^2.6.0",
    "unplugin-vue-markdown": "^0.25.2",
    "vue": "^3.0.0",
    "vue-markdown-render": "^2.1.1",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@quasar/app-vite": "^1.3.0",
    "@types/node": "^12.20.21",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.0.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4",
    "unplugin-auto-import": "^0.16.7",
    "unplugin-vue-components": "^0.25.2"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
